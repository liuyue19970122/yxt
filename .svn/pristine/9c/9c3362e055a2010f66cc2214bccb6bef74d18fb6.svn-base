<!--components/shadow-list/shadow-list.wxml-->
<!--components/account-item-border/acc-item-border.wxml-->
<view style='{{cusStyle}}'>
  <view class='account-item' bindtap='goTap' data-id='{{item.keyId}}' wx:for='{{shadowList}}'>
    <view wx:if='{{item.name}}'>{{item.name}}</view>
    <view wx:if='{{item.mobile}}'>{{item.mobile}}</view>
    <view wx:if='{{item.orgName}}'>{{item.orgName}}</view>
  </view>
</view>