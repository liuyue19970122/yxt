/* pages/system/charge-money/recharge.wxss */
.title{
  margin:40rpx 24rpx;
  color:#333;
  font-size:32rpx;
}
.money-list{
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  padding:0 24rpx;
  justify-content: space-between;
}
.money-item{
  width:220rpx;
  border-radius: 20rpx;
  height:100rpx;
  text-align: center;
  line-height: 100rpx;
  border:2rpx solid #f5f5f5;
  margin-bottom:20rpx;
  color:#999;
}
/* .money-item:not(:nth-child(3n+3)){
  margin-right:20rpx;
} */

.active-item{
  background: #fe2200;
  color:#fff;
}