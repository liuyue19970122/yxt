/* pages/receipt/vege/default-list/list.wxss */
@import '../../vage-list/list'
@import '../../../../utils/common'
.vage-item{
  width:726rpx;
  margin-left:24rpx
}
.vage-detail{
  width:558rpx;
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  align-items: center;
}
.vage-left-info{
  display: flex;
  justify-content: space-between;
  flex-direction: column;
  height:100%;
}
.form-item__content {
  width: 70%;
}
.picker{
  width:100%;
 display: flex;
  justify-content: flex-end;
  align-items: center;
}
.form{
  padding:0 16px;
}
.search-module{
  position: fixed;
  top:0;
  left:0;
  width:750rpx;
  background: #fff;
  box-shadow: 0 0 6rpx #CCC;
}
.default-list{
  margin-top:200rpx;
  /* padding-bottom:100rpx; */
}